	on edtFpsValue entered val do 
	(
		if ((val != ".") and (val as integer != undefined) and (val != "") and (val as integer >= 0)) then
		(
			valueFps = (val as integer)
		)
		else messagebox "---------------------------\r\nè¯·è¾“å…¥æ­£ç¡®å¸§ç‡æ•°å€¼\r\n"
	)
	on btnSetFps pressed do 
